{
  "name": "votery",
  "version": "0.0.0",
  "description": "A simple voting app written in nodeJS.",
  "keywords": [
    "express",
    "passport"
  ],
  "scripts": {
    "start": "webpack && nodemon ./server.js",
    "build": "webpack",
    "clean": "rm public/js/dist/* && rm public/css/*",
    "watch": "npm-scripts-watcher",
    "dev": "concurrently --kill-others 'yarn run watch' 'yarn run start'"
  },
  "watch": {
    "public/js/*.js": [
      "build"
    ],
    "sass/*.scss": [
      "build"
    ]
  },
  "author": "Adam Lance",
  "license": "Unlicensed",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.13.3",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.3.5",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "ejs": "^2.3.3",
    "ejs-locals": "^1.0.2",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "foundation-sites": "^6.4.4-rc1",
    "jquery": "^3.2.1",
    "mocha": "^4.0.1",
    "morgan": "^1.6.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npm-scripts-watcher": "^1.0.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "passport": "^0.2.2",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "sass-loader": "^6.0.6",
    "sortablejs": "^1.6.1",
    "style-loader": "^0.19.0",
    "underscore": "^1.8.3",
    "uuid": "^3.1.0",
    "what-input": "^5.0.3"
  },
  "devDependencies": {
    "webpack": "^3.8.1"
  }
}
